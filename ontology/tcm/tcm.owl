    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#numberOfOutboundDoors -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/latuli/ontology/tcm#numberOfOutboundDoors">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>The number of outbound doors available at a terminal.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#numberOfParkingBaysInBuffer -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/latuli/ontology/tcm#numberOfParkingBaysInBuffer">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
