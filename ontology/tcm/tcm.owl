    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#closeAdherenceToSchedule -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/latuli/ontology/tcm#closeAdherenceToSchedule">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
