    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#numberOfStockedProducts -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/latuli/ontology/tcm#numberOfStockedProducts">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Inventory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>The number of stocked products at a terminal</rdfs:comment>
    </owl:DatatypeProperty>
    


