<?xml version="1.0"?>
<rdf:RDF xmlns="http://latuli.no/terminalCapacityOnto#"
     xml:base="http://latuli.no/terminalCapacityOnto"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://w3id.org/latuli/ontology/tcm#">
        <owl:imports rdf:resource="http://www.opengis.net/ont/geosparql"/>
        <owl:imports rdf:resource="http://www.w3.org/2006/time#2016"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/latuli/ontology/tcm#containerTransitTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#containerTransitTime">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#dimensionMeasurementType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#dimensionMeasurementType">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Dimension"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#MeasurementType"/>
        <rdfs:comment>Relates a Dimension with a MeasurementType (e.g., metre, cubic or kilogram).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#handlingAreaSize -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#handlingAreaSize">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#Dimension"/>
        <rdfs:comment>Describes the size (physical characteristics) of the handling area in a terminal</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#hasMeasurementLimit -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#hasMeasurementLimit">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#MeasurementLimit"/>
        <rdfs:comment>Relates a PeriodicMeasurement to a MeasurementLimit in order to specify the type of measurement made (average, min, max, etc.)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#internalResourceCapacity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#internalResourceCapacity">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#Dimension"/>
        <rdfs:comment>The capacity of the conveyor belts network in an automated transportation mode, or the maximum number of workers available if the transportation is done manually, can be either considered limited or unlimited.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#internalTravelDistance -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#internalTravelDistance">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#Dimension"/>
        <rdfs:comment>Describes the distance from inbound docks to outbound docks including facilities used for processing the goods at a terminal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#loadingTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#loadingTime">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>The time it takes to load a transport means at the terminal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#missedOrders -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#missedOrders">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>The number of products that could not be loaded, or the corresponding lost profit.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#notFullyLoadedTransportMeans -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#notFullyLoadedTransportMeans">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>A ratio describing how often (on average) transport means depart from the terminal without full load</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#numberOfInboundTransportMeansProcessed -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#numberOfInboundTransportMeansProcessed">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>The number of inbound transport means processed in a given period of time</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#numberOfLoadingUnitsProcessed -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#numberOfLoadingUnitsProcessed">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>A measure specifying the number of loading units processed at a terminal in a given period of time</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#numberOfOutboundTransportMeansProcessed -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#numberOfOutboundTransportMeansProcessed">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>A measure specifying the number of loaded transport means (e.g. Trucks) processed at a terminal in a given period of time</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#numberOfTouches -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#numberOfTouches">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>Number of operations on a handling unit between it is unloaded from an inbound transport means and until it is loaded onto an outbound transport means</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#percentTrailersDeclinedEntrance -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#percentTrailersDeclinedEntrance">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>Describes the percentage of trailers declined entrance upon arrival</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#percentTransportMeansDeclinedEntrance -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#percentTransportMeansDeclinedEntrance">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>Describes the percentage of transport means declined entrance upon arrival</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#periodicMeasurementDurationType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#periodicMeasurementDurationType">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalUnit"/>
        <rdfs:comment>Describes a specific duration of a PeriodicMeasurement</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#planningHorizon -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#planningHorizon">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>The planning horizon (in time) used at a terminal</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#productType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#productType">
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#GoodsType"/>
        <rdfs:comment>Relates an Inventory with a GoodsType</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#productsStayTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#productsStayTime">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>The (average)  time spent inside the cross-dock for all the products.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#scheduleLength -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#scheduleLength">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>The time between completed operation and the beginning of a new one</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#serviceMode -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#serviceMode">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#ServiceMode"/>
        <rdfs:comment>Relates a terminal to a ServiceMode.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#stockedInventory -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#stockedInventory">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#Inventory"/>
        <rdfs:comment>Describes the number and type of stocked products at a terminal, possibly for a particular period of time.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#storageCapacity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#storageCapacity">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#Dimension"/>
        <rdfs:comment>Describes the capacity (physical characteristics) of the storage function in a terminal</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#supportedGoodsType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#supportedGoodsType">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#GoodsType"/>
        <rdfs:comment>Describes the types of goods supported by the terminal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#supportedLogisticsUnitType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#supportedLogisticsUnitType">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#LogisticsUnit"/>
        <rdfs:comment>Describes the types of logistics units supported at the terminal (e.g., containers, pallets, parcels, etc.)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#supportedTransportMeansType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#supportedTransportMeansType">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#TransportMeans"/>
        <rdfs:comment>Describes the types of transport means supported at the terminal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#supportedTransportMode -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#supportedTransportMode">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#TransportMode"/>
        <rdfs:comment>Describes the types of transport modes supported at the terminal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#terminalTotalSize -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#terminalTotalSize">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#Dimension"/>
        <rdfs:comment>Describes the total size (physical characteristics) of a terminal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#transhipmentTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#transhipmentTime">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>The processing time  (e.g. for a shipment or a consignment) from it arrives at the terminal until it leaves the terminal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#transitCenterSize -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#transitCenterSize">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#Dimension"/>
        <rdfs:comment>Describes the size (physical characteristics) of the transit center in a terminal</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#unloadingTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#unloadingTime">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>The time it takes to unload a transport means at the terminal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#waitingTimeInboundTransportMeans -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#waitingTimeInboundTransportMeans">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>Time spent at docks waiting to unload goods.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#waitingTimeOutboundTransportMeans -->

    <owl:ObjectProperty rdf:about="https://w3id.org/latuli/ontology/tcm#waitingTimeOutboundTransportMeans">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="https://w3id.org/latuli/ontology/tcm#PeriodicMeasurement"/>
        <rdfs:comment>Time spent at docks waiting to transport goods from terminal.</rdfs:comment>
    </owl:ObjectProperty>
    


