    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#dimensionMeasurementValue -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/latuli/ontology/tcm#dimensionMeasurementValue">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Dimension"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>A value that specifies a dimension measurement (e.g. The total size of a terminal has a value of 1000 (square metres)).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#doorUtilisationRate -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/latuli/ontology/tcm#doorUtilisationRate">
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Describes the occupation time of doors at the terminal.</rdfs:comment>
    </owl:DatatypeProperty>
    


