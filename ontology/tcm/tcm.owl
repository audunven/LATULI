    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/latuli/ontology/tcm#averageNumberOfWorkingHours -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/latuli/ontology/tcm#averageNumberOfWorkingHours">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
