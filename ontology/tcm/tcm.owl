    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/latuli/ontology/tcm#numberOfTrailerParkingBays -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/latuli/ontology/tcm#numberOfTrailerParkingBays">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/latuli/ontology/tcm#Terminal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
